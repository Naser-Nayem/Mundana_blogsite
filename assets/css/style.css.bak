/*header part start*/
.header{}
.header-logo{}
.header-nav{}
.header-nav li a{
	transition: 0.5s;
}
.header-nav li a:hover{
	color: #000000;
	
}
.header-icon{}
.header-icon button:hover{
	background-color: #03A87C;
	color: #ffffff;
	cursor: pointer;
}
/*header part end*/
/*banner part start*/
.banner-text a{
	opacity: 0.8;
	transition: .3s;
}
.banner-text a:hover{
	opacity: 1;
}
/*banner part end*/
/*section 1 start*/
.image-des a{
	transition: 0.5s;
}
.image-des a:hover{
	color: #4bad64;
}
.des a{
	transition: 0.5s;
}
.des a:hover{
	color: #4bad64;
	text-decoration: underline;
}
/*section 1 end*/

/*section 3 start*/
.sec-3-text a{
	transition: 0.5s;
}
.sec-3-text a:hover{
	color: #4bad64;
	text-decoration: underline;
}
.sec-3-side-font-main a{
	transition: 0.5s;
}
.sec-3-side-font-main a:hover{
	color: #4bad64;
	text-decoration: underline;
}
.footer-right a{
	transition: 0.5s;
}
.footer-right a:hover{
	text-decoration: underline;
}
/*section 3 end*/

